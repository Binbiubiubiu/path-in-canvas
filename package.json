{
  "name": "path-in-canvas",
  "version": "0.0.5",
  "description": "use path's data in the canvas",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.esm.js",
  "typings": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "dev": "nodemon --exec \"father build\"",
    "build": "father-build",
    "test": "jest",
    "release:preview": "np --no-publish --preview",
    "release": "np --no-publish --no-yarn"
  },
  "keywords": [
    "svg",
    "canvas",
    "path",
    "transform"
  ],
  "author": "Simon-Bin",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.2",
    "babel-jest": "^27.3.1",
    "father-build": "^1.20.5-0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "np": "^7.5.0",
    "typescript": "^4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simon-Bin/path-in-canvas.git"
  },
  "bugs": {
    "url": "https://github.com/Simon-Bin/path-in-canvas/issues"
  },
  "homepage": "https://github.com/Simon-Bin/path-in-canvas#readme"
}
